{
  "name": "<%= dasherize(name) %>.service",
  "version": "1.0.0",
  "description": "<%= dasherize(name) %>.service, an awesome fun.framework project",
  "main": "app.js",
  "scripts": {
    "dev-start": "node start",
    "test": "NODE_ENV=test jest --runInBand --passWithNoTests",
    "local-db:reset": "NODE_ENV=local sequelize db:migrate:undo:all && NODE_ENV=local sequelize db:migrate && NODE_ENV=local sequelize db:seed:all",
    "dev-db:reset": "sequelize db:migrate:undo:all && sequelize db:migrate && sequelize db:seed:all",
    "test-db:reset": "NODE_ENV=test sequelize db:migrate:undo:all && NODE_ENV=test sequelize db:migrate && NODE_ENV=test sequelize db:seed:all",
    "docs": "npx apidoc -i ./src/ -o ./src/docs/"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "jest": {
    "testEnvironment": "node",
    "setupFilesAfterEnv": [
      "./tests/jest.setup.js"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ],
    "testSequencer": "./tests/jest.sequencer.js",
    "globalSetup": "./tests/jest.setup.js",
    "globalTeardown": "./tests/jest.teardown.js"
  },
  "author": "fun.framework CLI",
  "license": "MIT",
  "dependencies": {
    "auto-bind": "^4.0.0",
    "axios": "^0.21.1",
    "bcrypt": "^5.0.1",
    "cors": "^2.8.5",
    "cron": "^1.8.2",
    "cron-validator": "^1.1.1",
    "ejs": "^3.1.6",
    "express": "^4.17.1",
    "express-formidable": "^1.2.0",
    "fun.framework": "^0.4.2",
    "joi": "^17.4.2",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.29.1",
    "mysql2": "^2.1.0",
    "nodemailer": "^6.5.0",
    "ramda": "^0.27.0",
    "random-key": "^0.3.2",
    "sequelize": "^6.6.5",
    "socket.io": "^4.1.3",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "jest": "^27.0.6",
    "sequelize-cli": "^6.2.0",
    "supertest": "^6.1.4",
    "apidoc": "^0.29.0"
  },
  "apidoc": {
    "name": "<%= dasherize(name) %>.service"
  }
}
